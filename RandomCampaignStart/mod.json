  "Settings": {
    /* GLOBAL TOGGLES*/
    /*NotRandomMode: false allows you to select your exact starting lance, false for Legacy Method Randomization.*/
    "NotRandomMode": false, //If TagRandomLance set to true this toggle changes nothing.

    /*TagRandomLance: true to have Starting Lance consist of local Mech variants; false to allow one of the other methods: FullRandom or LegacyRandom*/
    "TagRandomLance": true,

    /* Global toggles that effect all types of randomizers.*/
    "MinimumStartingWeight": 210,
    "MaximumStartingWeight": 240,
    "NumberRandomRonin": 7,
    "NumberProceduralPilots": 0,
    "MechPercentageStartingCost": 1,
    "IgnoreAncestralMech": false,

    /* Settings for Original Method. Use this method to input specific mechs to be randomized amongst. Also, this allows precise control of mech weight classes. */
    "RemoveAncestralMech": false,

    /* Specify which Ronin you want to have in your game to start. NumberRoninFromList will specify how many Ronin will be randomly selected from amongst this list. */
    "NumberRoninFromList": 0,
    "StartingRonin": [
      "pilot_sim_starter_medusa",
      "pilot_sim_starter_behemoth",
      "pilot_sim_starter_dekker",
      "pilot_sim_starter_glitch"
    ],

    /* Settings for Full Random Mode. Maximum Lance Size = 6.*/
    "MinimumLanceSize": 4,
    "MaximumLanceSize": 6,
    "MaximumMechWeight": 50,
    "AllowDuplicateChassis": false,
    "AllowCustomMechs": false,

