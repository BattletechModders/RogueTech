      },
      "name": "Dialogue_Interrupt_GoToComplete",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "You should have a clear view of the {TEAM_TAR.FactionDef.Demonym} facility now, Commander.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
