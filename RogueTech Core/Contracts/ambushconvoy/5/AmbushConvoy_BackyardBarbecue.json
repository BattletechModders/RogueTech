      "name": "Dialogue_MissionStart",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "According to these survey maps, you should be able to see the convoy's route from this location.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "cb50e1e4-aa0c-49b2-bd2e-5339c78dafb5",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Once you have eyes on them, destroy the vehicles. Any other units are at your discretion, but there shouldn't be much opposition.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "cb50e1e4-aa0c-49b2-bd2e-5339c78dafb5",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Famous last words. Commander, when you're ready for pickup, I'll meet you at this LZ.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SumireDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "a85da736-7a2e-4cc6-aff3-feb9d73fc7e1",
