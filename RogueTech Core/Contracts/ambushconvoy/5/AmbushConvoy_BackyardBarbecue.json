        "EncounterObjectGuid": "a50ad243-225a-4beb-9317-d7aeffa2bcdc"
      },
      "name": "Dialogue_Interrupt_GuardsDestroyed",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "That was an awful lot of metal on the field for 'not much opposition.'",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SumireDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Seems like our employers may have a leak.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "I'm not seeing any more 'Mech signatures, at least, so that's money in the bank.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SumireDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Only if we take out that convoy, Meyer.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
