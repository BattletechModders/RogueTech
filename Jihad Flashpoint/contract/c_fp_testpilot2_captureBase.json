      {
         "dialogue": {
            "EncounterObjectGuid": "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
         },
         "name": "Dialogue_MissionStart",
         "overrideDialogueBucketId": "",
         "dialogueContent": [
            {
               "words": "This is the target area, Commander. The facility is just up ahead.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_FP_Duchess1",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            },
            {
               "words": "We're expecting heavy defenses, including fixed gun emplacements.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_FP_Duchess1",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "181efcdf-0e34-477b-8a06-39b0ead2d69e",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            },
            {
               "words": "Occupy the facility and destroy any forces you encounter.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_FP_Duchess1",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "76b654a6-4f2c-4a6f-86e6-d4cf868335fe",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            }
         ]
      },
      {
         "dialogue": {
            "EncounterObjectGuid": "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
         },
         "name": "Dialogue_MissionSuccess",
         "overrideDialogueBucketId": "",
         "dialogueContent": [
            {
               "words": "The Yurei performed as expected.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_FP_Duchess1",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            },
            {
               "words": "We're glad to be of assistance.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_DariusDefault",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            },
            {
               "words": "Return to the Argo, I will be sending coordinates of one of our besieged bases shortly.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_FP_Duchess1",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            },
            {
               "words": "Understood, Duchess. I'll inform the rest of the crew.",
               "wordsColor": {
                  "r": 1,
                  "g": 1,
                  "b": 1,
                  "a": 1
               },
               "selectedCastDefId": "castDef_DariusDefault",
               "emote": "Default",
               "audioName": "NONE",
               "cameraFocusGuid": "",
               "cameraDistance": "Far",
               "cameraHeight": "Default",
               "revealRadius": -1
            }
         ]
      },
