{
  "Name": "CrystalClear",
  "Version": "2.5.2",
  "Enabled": true,
  "DLL": "CrystalClear.dll",

  "ConflictsWith": [ ],

  // Settings must be in caps
  // NOTSET just leaves the game to run vanilla
  // otherwise ON or OFF, eg "Grain": "OFF",

  "Settings": {
    // reduces clarity
    "Grain": "OFF",

    // reduces clarity
    "Dithering": "OFF",

    // darkens the edges of the scene
    "Vignette": "OFF",

    // makes light sources overblown when enabled
    "Bloom": "OFF",

    // make UI elements glow
    "UIBloom": "OFF",
    "Grunge": "OFF",
    "Scanlines": "OFF",
    "DustStorms": "OFF",
    "TurnBanner": "OFF",
    "MotionBlur": "OFF",
    "Shadows": "NOTSET",

    // unsure if used.  it should appear as green/purple edge tinting
    "ChromaticAberration": "OFF",

    // simulates pupil dilation response
    "EyeAdaptation": "OFF",

    // setting to true makes everywhere foggy
    "Fog": "OFF",

    // unsure
    "ColorGrading": "OFF",

    // looks so good I wouldn't disable it
    "AmbientOcclusion": "NOTSET",

    // temporal antialiasing aka MFAA - good fidelity
    "Taa": "NOTSET",
    "DepthOfField": "OFF",
    "Fxaa": "NOTSET",

    // renderer is expecting this to be on but works without
    "HDR": "NOTSET",
    "ChromaticAberration": "NOTSET",
    "EyeAdaptation": "NOTSET",
    "Fog": "OFF",
    "ColorGrading": "NOTSET"
  }
}
