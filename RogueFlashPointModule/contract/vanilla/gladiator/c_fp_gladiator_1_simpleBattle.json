    "dialogueList" : [
        {
            "dialogue" : {
                "EncounterObjectGuid" : "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "name" : "Dialogue_MissionStart",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "Our surveillance drones have spotted a group of the Steel Beast's friends and followers near this location.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_SteinerRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Understand that these people aren't trained mercenaries. They're a motley crew of Solaris gladiators, pirates, and highwaymen. Most don't belong on the battlefield at all, let alone in a BattleMech's cockpitâ€¦ it should be a trivial matter for you to smash them flat.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_SteinerRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "d9a28c08-abdb-42bb-a140-ad3927d1f8cf",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Sounds like a plan, Graf Stieglitz-Bradford. {COMMANDER.FirstName}, I'll be recording this fight for broadcast, so try to put on a good show. Give 'em the business. Oliveira out.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
            },
            "name" : "Dialogue_MissionSuccess",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "That was glorious, Commander {COMMANDER.LastName}! You beat them bloody without a shred of hesitation or remorse!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_SteinerRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "And I captured the entire show on Holovid! Nice, crisp footage, ready for air.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Then by all means, we should broadcast it. We move on Ironwood Productions and their Holo-distribution facility, and we do it now!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_SteinerRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "What about refits and repairs? Our lance just took fire down there.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_YangDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "I am paying you to deliver swift justice for Arjun's murder, Mr. Virtanen. Anything short of that is unacceptable. You will move on Ironwood's Holo-distribution facility, and you will do it without delay.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_SteinerRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "You're the boss, Graf Stieglitz-Bradford. If you say we push forward, then we push forward.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "d3d33d95-9ed7-4686-b9eb-954ebe51cc02"
            },
            "name" : "Dialogue_MissionFailure",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "I don't know how we lost this, but hey. Shit happens. We're pulling out, this Flashpoint is over.",
                    "wordsColor" : {
